body {
	background-color: #8ecae6;
	color: #f1faee;
	font-family: 'Trebuchet MS', sans-serif;
}

h1 {
	color: #023047;
}

h2 {
	color: #ffb703;
}

div {
	background-color: #219ebc;
}

.navbar {
	list-style-type: none;
	margin: 0;
	padding: 0;
	overflow: hidden;
	background-color: #219ebc;
	position: sticky;
	top: 0;
	width: 100%;
}

.navli {
	display: inline;
	padding: 75px 0 75px 0;
	text-align: center;
}

.container {
	display: flex;
	align-content: space-around;
	margin: 10px 0 10px 0;
}

.item {
	padding: 10px 10px 10px 10px;
}

.profpic {
	border-radius: 100px 100px 100px 100px;
	height: 200px;
	width: 200px;
	
}

.profile {
	background-color: #023047;
	flex: 1 1 auto;
}

.summaryme {
	background-color: #219ebc;
	flex: 2 1 500px;
}

footer {
	text-align: center;
	font-size: 10px;
}